{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "BUSINESS_NAME": "Jims Gym",
    "TEMPLATE_ID": "gym-modern",
    "MODULES": "Gallery,booking,social",
    "THEME": "Light"
  }
}